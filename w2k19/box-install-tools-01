choco install 7zip.install              --yes -limitoutput --no-progress
choco install git                       --yes -limitoutput --no-progress
choco install ConEmu                    --yes -limitoutput --no-progress
choco install freecommander-xe.install  --yes -limitoutput --no-progress

choco install notepadplusplus                            --yes -limitoutput --no-progress
choco install vscode           --params "/NoDesktopIcon" --yes -limitoutput --no-progress

choco install googlechrome  --yes -limitoutput --no-progress
choco install firefox       --params "/NoDesktopShortcut" --yes -limitoutput --no-progress
